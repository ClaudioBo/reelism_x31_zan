// Tecmo Super Bowl quarterbacks (hut hut hut hut hut hut hut hut hut hut hut)
ACTOR Quarterback : MonsterTemplate
{
	Health 5
	Height 48
	Radius 24
	Scale 3
	Speed 24
	PainChance 32
	PainSound "brony/pain"
	SeeSound "qback/ready"
	DeathSound "qback/down"
	ActiveSound "qback/hut"
	Obituary "$OB_QUARTERBACK"
	HitObituary "$OB_QUARTERBACKHIT"
	//DropItem ""
	States
	{
	Spawn:
		QBAK A 0
		QBAK A 0 Thing_ChangeTID(0,900)
		QBAK A 1 A_Look
		QBAK A 0 A_AlertMonsters
		Goto See
	See:
		// Check for Dog Whistle, UNLESS it's 9-9-9, since they're only spawned during that jackpot and it doesn't make sense to confuse them right off the bat
		TNT1 A 0 A_JumpIf(ACS_ExecuteWithResult(901)==9 && !(ACS_ExecuteWithResult(902)==9 && ACS_ExecuteWithResult(903)==9),"DogWhistle")
		// Otherwise, run
		QBAK A 3 A_Chase("Melee", "Missile", CHF_NOPLAYACTIVE)
		QBAK A 0 A_ActiveSound
		QBAK A 0 A_JumpIf(velz!=0,"MidAir")
		QBAK B 3 A_Chase("Melee", "Missile", CHF_NOPLAYACTIVE)
		QBAK B 0 A_JumpIf(velz!=0,"MidAir")
		QBAK C 3 A_Chase("Melee", "Missile", CHF_NOPLAYACTIVE)
		QBAK C 0 A_JumpIf(velz!=0,"MidAir")
		QBAK D 3 A_Chase("Melee", "Missile", CHF_NOPLAYACTIVE)
		QBAK D 0 A_ActiveSound
		QBAK D 0 A_JumpIf(velz!=0,"MidAir")
		QBAK E 3 A_Chase("Melee", "Missile", CHF_NOPLAYACTIVE)
		QBAK E 0 A_JumpIf(velz!=0,"MidAir")
		QBAK F 3 A_Chase("Melee", "Missile", CHF_NOPLAYACTIVE)
		Loop
	MidAir:
		QBAK AAAAAA 1 A_CheckFloor("See")
		TNT1 A 0 A_SpawnItemEx("CrushDamage")
		QBAK A 1 A_CheckFloor("Landing")
		TNT1 A 0 A_JumpIf(velz==0,"See")
		goto MidAir+6
	Landing:
		QBAK A 1
		QBAK A 0 A_SpawnItemEx("LandingPart",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION)
		QBAK A 5 A_PlaySound("human/land")
		goto See
	Melee:
	Missile:
		QBAK G 0 A_PlaySound("qback/ready")
		QBAK GG 8 A_FaceTarget
		QBAK H 0 A_CustomComboAttack("HandEgg", 12, 10, "attack/punch")
		QBAK H 8 A_PlaySound("qback/down")
		Goto See
	Pain:
		QBAK I 0 A_Pain
		QBAK I 3 A_Chase("", "")
		Goto See
	// NORMAL DEATH STATES
	Death:
		TNT1 A 0 A_JumpIf(ACS_ExecuteWithResult(901)==8,"BirthdayParty")
		QBAK I 0 A_NoBlocking
		QBAK I 0 A_GiveToTarget("ScoreItem",100)
		QBAK I 1 A_Scream
		goto CorpseFalling
	CorpseFalling:
		QBAK IIIIII 1 A_CheckFloor("Corpse")
		QBAK I 1 A_CheckFloor("CorpseHard")
		TNT1 A 0 A_JumpIf(velz==0,"Corpse")
		goto CorpseFalling+6
	Corpse:
		QBAK I 4 A_PlaySound("human/body")
		QBAK J 4
		QBAK K 10 A_JumpIf(velz!=0,"CorpseFalling")
		TNT1 A 0 A_CheckSight("Cleanup")
		QBAK L 10 A_JumpIf(velz!=0,"CorpseFalling")
		TNT1 A 0 A_CheckSight("Cleanup")
		goto Corpse+2
	CorpseHard:
		QBAK I 0 A_SpawnItemEx("LandingPart",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION)
		goto Corpse
	// COMEDY DEATH STATES
	CorpseFlying:
		QBAK I 0 A_GiveToTarget("ScoreItem",1)
		QBAK I 0 A_SpawnItemEx("BloodShredSilent")
		QBAK I 0 A_SpawnItemEx("MissilePart",0,0,8)
		QBAK I 0 A_JumpIf(momx == 0,"CorpseCrater")
		QBAK I 0 A_JumpIf(momy == 0,"CorpseCrater")
		QBAK I 1 A_CheckFloor("CorpseCrater")
		loop
	CorpseCrater:
		QBAK I 0 A_SpawnItemEx("LandingPart",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION)
		QBAK I 0 A_SpawnItemEx("HugeBloodShred",0,0,16)
		QBAK I 0 A_PlaySound("human/gib")
		QBAK I 0 A_GiveToTarget("ScoreItem",50)
		TNT1 A 0 A_JumpIf(ACS_ExecuteWithResult(901) ==8,"BirthdayParty")
		QBAK I 0 A_Stop
		goto Corpse+1
	Death.Fire:
		QBAK A 0 A_PlaySound("human/scream")
		QBAK A 0 A_SpawnItemEx("Flame1A",0,0,32)
		QBAK A 0 A_Recoil(-8)
		QBAK A 0 A_SpawnItemEx("Ember1",0,0,32)
		QBAK AABBCC 1 A_SetAngle(12 + angle)
		QBAK B 0 A_Recoil(-8)
		QBAK A 0 A_SpawnItemEx("Flame2A",0,0,32)
		QBAK DDEEFF 1 A_SetAngle(12 + angle)
		QBAK C 0 A_Recoil(-8)
		QBAK A 0 A_SpawnItemEx("Flame1A",0,0,32)
		QBAK AABBCC 1 A_SetAngle(12 + angle)
		QBAK D 0 A_Recoil(-8)
		QBAK DDEEFF 1 A_SetAngle(12 + angle)
		QBAK A 0 A_SpawnItemEx("Flame3B",0,0,32)
		QBAK A 0 A_Jump(128,"Death.Fire")
		goto Death
	DogWhistle:
		TNT1 A 0 A_SpawnItemEx("Confusion",0,0,56)
		QBAK A 3 A_Wander
		QBAK A 0 A_JumpIf(velz!=0,"MidAir")
		QBAK B 3 A_Wander
