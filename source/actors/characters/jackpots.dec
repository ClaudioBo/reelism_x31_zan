ACTOR CyberdemonGun : Weapon
{
	Weapon.SelectionOrder 2500
	+WEAPON.NOAUTOFIRE
	+INVENTORY.UNDROPPABLE
	//+INVENTORY.UNTOSSABLE
	Tag "$TAGCYB"
	States
	{
	Ready:
		TNT1 A 0 A_StopSound(6)
		CARM A 1 A_WeaponReady
		Loop
	Deselect:
		CARM A 1 A_Lower
		Loop
	Select:
		CARM A 1 A_Raise
		Loop
	Fire:
		CARM B 0 A_GunFlash
		CARM B 2 bright A_FireCustomMissile("Missile",0,0,0,32)
		CARM CDEF 2
		CARM A 12
		CARM A 0 A_ReFire
		Goto Ready
	Flash:
		TNT1 A 3 A_Light1
		TNT1 A 4 A_Light2
		Goto LightDone
	Spawn:
		LAUN A 1 bright
		TNT1 A 0 A_SpawnItemEx("DespawnCloudPart",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION)
		stop
	}
}

