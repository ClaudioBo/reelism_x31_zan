// bitman jumpers!
ACTOR 8Jumper : MonsterTemplate
{
  Health 50
  PainChance 180
  Speed 4
  Radius 15
  Height 56
  Mass 400
  seesound "8enemy/see"
  AttackSound "8enemy/melee"
  painsound "8enemy/hurt"
  deathsound "8enemy/die"
  Obituary "$OB_JUMPER"
  States
  {
  Spawn:
	JMPR A 0
	JMPR A 0 Thing_ChangeTID(0,900)
	JMPR A 1 A_Look
	JMPR A 0 A_AlertMonsters
    Loop
  See:
    JMPR A 0
    JMPR A 0 A_PlaySound("8jumper/land")
    goto Stand
  Stand:
	JMPR A 0 
    JMPR A 0 Thing_Stop(0)
    JMPR A 2 A_Chase
    JMPR A 0 A_Jump(128, "Stand")
    JMPR A 1 
    goto Bounce
  Bounce:
    JMPR B 0 
    JMPR B 0 A_PlaySound("8jumper/jump")
    JMPR B 1 A_Chase
    JMPR B 1 ThrustThingZ(0,40,0,0)
    JMPR B 1 ThrustThing(angle*256/360, 10, 0, 0)
    goto Midair
  Midair:
    JMPR B 1
    JMPR B 0 A_JumpIf(z == floorz, "See")
    Loop
  Melee:
	JMPR C 4 A_FaceTarget
	JMPR D 3 A_CustomMeleeAttack(3,"8jumper/melee", "8jumper/melee", Enemy)
	JMPR C 3 A_FaceTarget
	JMPR D 8 A_CustomMeleeAttack(3,"8jumper/melee")
	goto See
  Pain:
    JMPR E 2
    JMPR E 2 A_Pain
    Goto See
  Death:
	TNT1 A 0 A_JumpIf(ACS_ExecuteWithResult(901) ==4,"XDeath")
	TNT1 A 0 A_JumpIf(ACS_ExecuteWithResult(901) ==8,"BirthdayParty")
    JMPR E 10 A_Scream
    JMPR F 10 A_NoBlocking
    goto CorpseFalling
	CorpseFalling:
		JMPR EFEFEF 1 A_CheckFloor("Corpse")
		JMPR F 1 A_CheckFloor("CorpseHard")
		TNT1 A 0 A_JumpIf(velz==0,"Corpse")
		goto CorpseFalling+6
	Corpse:
		JMPR F 1 A_PlaySound("human/body")
		JMPR F 1 A_JumpIf(velz!=0,"CorpseFalling")
		TNT1 A 0 A_CheckSight("Cleanup")
		goto Corpse+1
	CorpseHard:
		JMPR F 0 A_SpawnItemEx("LandingPart",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION)
		goto Corpse
	// GIB DEATH STATES
	XDeath:
		TNT1 A 0 A_JumpIf(ACS_ExecuteWithResult(901) ==8,"BirthdayParty")
		JMPR E 0 A_NoBlocking
		JMPR E 0 A_GiveToTarget("ScoreItem",130)
		TNT1 A 0 A_StopSound(CHAN_VOICE)
		JMPR E 0 A_PlaySound("human/gib")
		JMPR E 0 A_SpawnItemEx("HugeBloodShred",0,0,16)
		JMPR F 5 A_SpawnItemEx("HugeBloodShred",0,0,16)
		TNT1 A 0 A_SetScale(1)
		GIBS MNO 5 A_SpawnItemEx("HugeBloodShred",0,0,16)
		GIBS PQRST 4
		GIBS U 5
		GIBS V 1400
		Stop
	// COMEDY DEATH STATES
	Death.Explosive:
		JMPR E 0 A_Jump(64,"XDeath")
